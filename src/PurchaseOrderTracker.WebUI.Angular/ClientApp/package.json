  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod=true",
    "test": "ng test",
    "test:single": "ng test --watch=false",
    "test:features": "cucumber-js",
    "test:features-headless": "set headless=true && cucumber-js",
    "lint": "npm run lint:ts && npm run lint:styles && npm run lint:html",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:html": "prettier --check \"src/**/*.html\" || exit 0",
    "d": "d ned to update these scripts + docker build to run them",
    "clean": "rimraf target",
    "test:update-serenity": "serenity-bdd update --ignoreSSL",
    "test:acceptance": "protractor ./protractor.conf.js",
    "test:report": "serenity-bdd run",
    "test2": "failsafe clean test:update-serenity test:acceptance test:report",
    "verify": "npm test"
  },
